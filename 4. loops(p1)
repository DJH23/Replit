var s = "aaaaa"

i = 0
j = s.length - 1
var palindrome = true;
while (i <= j) {
  console.log(s[i], s[j]);
  if (s[i] != s[j]) {
    palindrome = false;
    break;
  }
  i++;
  j--;
}

console.log(palindrome);